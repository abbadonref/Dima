import java.util.Scanner;

public class Main {

    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String str=sc.next();
        int s1=0;

        int b=str.length();
        for (int i=0; i<b; i++){
            if (str.charAt(i) =='3')
            { s1++;
            }

        }
        System.out.println(s1);



    }
}
