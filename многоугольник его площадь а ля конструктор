package geom;

public class mnogoygolnik {
    private int x;
    private int y;
    public mnogoygolnik(){
            x=1;
            y=1;
        }

        public  mnogoygolnik(int x1, int x2)
        {this.x=x1;this.y=x2;}

        public String getSquare(){
        return "na tebe ploshadi " + (x*y);
        }

    public String getX (){
        return "coordinate x" + x;
    }
    public String getY (){
        return "coordinate y" + y;

    }}
