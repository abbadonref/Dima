import java.util.Scanner; // импорт класса Scanner

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        System.out.println("enter numbers ");
        int [] array=new int[10];
      int i;
        int sum=0;
      for(i=0;i<10;i++){
          array[i]= scn.nextInt();
          sum=sum+array[i];
      }
        System.out.println(sum);
      }}
